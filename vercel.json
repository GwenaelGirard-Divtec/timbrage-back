{
    "version": 2,
    "builds": [
        { "src": "/dist/index.php", "use": "vercel-php@0.5.2" },
        { "src": "/public/**", "use": "@vercel/static" }
    ],
    "routes": [
        {
            "src": "/(css|js|images)/(.*)",
            "dest": "public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/index.php"
        }
    ],
    "env": {

        "APP_NAME": "Lumen",
        "APP_ENV": "local",

        "APP_DEBUG": "true",
        "APP_TIMEZONE": "UTC",

        "DB_CONNECTION": "mysql",
        "DB_HOST": "eq8z1.myd.infomaniak.com",
        "DB_PORT": "3306",
        "DB_DATABASE": "eq8z1_timbrage",
        "DB_USERNAME": "eq8z1_adm_timbra",
        "DB_PASSWORD": "N8-RdufcxN9",

        "QUEUE_CONNECTION": "sync",


        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",

        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
}